  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "blob-util": "^2.0.2",
    "chalk": "^4.0.0",
    "debug": "^4.3.3",
    "mock-socket": "^9.1.5",
    "url": "^0.11.0",
    "uuid": "^9.0.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "isomorphic-ws": "^5.0.0",
    "vitest": "^0.29.8"
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.ts",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "test": "vitest --run"
  }
