{
  "name": "@fakehost/host",
  "description": "Create fake websocket services that can run as a service or bundled within the browser.",
  "keywords": ["fake", "websocket", "testing"],
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/ilikejames/fakehost",
  "main": "dist/index.cjs",
  "type": "module",
