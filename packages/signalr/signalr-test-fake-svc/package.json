{
    "name": "@fakehost/signalr-test-fake-svc",
    "description": "Fake versions of the local dotnet test service",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.cjs",
    "type": "module",
    "exports": {
        ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
        }
    },
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "dependencies": {
        "@fakehost/signalr": "^1.0.0",
        "@fakehost/signalr-test-client-api": "^1.0.0"
    },
    "devDependencies": {
        "@microsoft/signalr": "^7.0.5",
        "cross-env": "^7.0.3",
        "rxjs": "^7.4.0",
        "vitest": "^0.29.8"
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
        "lint": "eslint --ext .tsx --ext .ts src/",
        "test": "cross-env TEST_TARGET=FAKE vitest --run",
        "test:contract": "cross-env TEST_TARGET=REMOTE vitest --run"
    }
}