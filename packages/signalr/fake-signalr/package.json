{
  "name": "@fakehost/signalr",
  "description": "A Fake Signalr Service for faking/mocking signalr hub services for testing, prototyping, and demoing",
  "keywords": [
    "fake",
    "signalr",
    "mock",
    "testing",
    "storybook",
    "contact"
  ],
  "version": "1.0.0-beta.3",
  "private": false,
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs"
      },
      "default": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    }
  },
  "browser": {
    "./index.js": "./dist/browser.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@fakehost/exchange": "^1.0.0-beta.2",
    "@fakehost/fake-rest": "^1.0.0-beta.3",
    "@microsoft/signalr-protocol-msgpack": "^7.0.7",
    "@msgpack/msgpack": "^2.8.0",
    "url": "^0.11.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@microsoft/signalr": "^7.0.5",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "rxjs": "^7.4.0",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "build": "tsup-node --config ./tsup.config.ts",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "typecheck": "tsc --noEmit"
  }
}
