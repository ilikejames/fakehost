{
    "name": "@fakehost/signalr",
    "description": "A Fake Signalr Service for faking/mocking signalr hub services for testing purposes",
    "keywords": ["fake", "signalr", "mock", "testing", "storybook", "react-native", "contact"],
    "version": "1.0.0",
    "private": false,
    "main": "dist/index.cjs",
    "type": "module",
    "exports": {
        ".": {
          "types": "./dist/index.d.ts",
          "import": "./dist/index.js",
          "require": "./dist/index.cjs"
        },
        "./browser": {
          "types": "./dist/createInBrowser.d.ts",
          "import": "./dist/createInBrowser.js",
          "require": "./dist/createInBrowser.cjs"
        },
        "./server": {
          "types": "./dist/createServer.d.ts",
          "import": "./dist/createServer.js",
          "require": "./dist/createServer.cjs"
        },
        "./package.json": "./package.json"
      },
      "browser": {
        "./browser": "./createInBrowser.js"
      },
      "typesVersions": {
        "*": {
          "*": ["./dist/index.d.ts"],
          "browser": ["./dist/createInBrowser.d.ts"],
          "server": ["./dist/createServer.d.ts"]
        }
      },
    "files": [
        "dist"
    ],
    "license": "MIT",
    "dependencies": {
        "@fakehost/exchange": "^0.2.0",
        "@fakehost/fake-rest": "^1.0.0",
        "@fakehost/host": "^1.0.0",
        "uuid": "^9.0.0",
        "url": "^0.11.0",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@fakehost/signalr-test-client-api": "*",
        "@microsoft/signalr": "^7.0.5",
        "@types/uuid": "^9.0.1",
        "@types/ws":"^8.5.4",
        "chalk": "^5.2.0",
        "cross-env": "^7.0.3",
        "rxjs": "^7.4.0",
        "vitest": "^0.29.8"
    },
    "scripts": {
        "build": "tsup --config ./tsup.config.ts",
        "lint": "eslint --ext .tsx --ext .ts src/"
    }
}