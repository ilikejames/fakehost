{
  "name": "@fakehost/rest",
  "description": "A fake REST server that can run as a service or bundled within a browser, for testing and demoing",
  "keywords": ["fake", "rest", "browser", "react-native", "mock"],
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ilikejames/fakehost",
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chalk": "^4.0.0",
    "debug": "^4.3.3",
    "path-to-regexp": "^6.2.1",
    "url": "^0.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^14.0.0",
    "@types/uuid": "^8.3.4",
    "isomorphic-fetch": "^3.0.0",
    "typescript": "^4.5.5",
    "vitest": "^0.29.8"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --splitting",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "test": "vitest --run"
  }
}
