{
  "name": "@fakehost/fake-rest",
  "description": "A fake REST server that can run as a service or bundled within a browser, for testing and demoing",
  "keywords": ["fake", "rest", "browser", "react-native", "mock"],
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ilikejames/fakehost",
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/HijackedRestService.d.ts",
      "import": "./dist/HijackedRestService.js",
      "require": "./dist/HijackedRestService.cjs"
    },
    "./server": {
      "types": "./dist/HttpRestService.d.ts",
      "import": "./dist/HttpRestService.js",
      "require": "./dist/HttpRestService.cjs"
    },
    "./package.json": "./package.json"
  },
  "browser": {
    "./browser": "./HijackedRestService.js"
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/index.d.ts"],
      "browser": ["./dist/HijackedRestService.d.ts"],
      "server": ["./dist/HttpRestService.d.ts"]
    }
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "chalk": "^4.0.0",
    "debug": "^4.3.3",
    "isomorphic-fetch": "^3.0.0",
    "path-to-regexp": "^6.2.1",
    "streams": "^0.0.11",
    "url": "^0.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^14.0.0",
    "@types/uuid": "^8.3.4",
    "typescript": "^5.0.0",
    "vitest": "^0.29.8"
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.ts",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "test": "vitest --run"
  }
}
