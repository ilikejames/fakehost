{
  "name": "@fakehost/fake-rest",
  "description": "A fake REST server that can run as a service or bundled within a browser, for testing and demoing",
  "keywords": [
    "fake",
    "rest",
    "browser",
    "react-native",
    "mock"
  ],
  "version": "1.0.0-beta.2",
  "private": false,
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/ilikejames/fakehost",
  "main": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./package.json": "./package.json"
  },
  "browser": {
    "./browser": "./dist/browser.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "browser": [
        "./dist/browser.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ]
    }
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "chalk": "^4.0.0",
    "debug": "^4.3.3",
    "isomorphic-fetch": "^3.0.0",
    "path-to-regexp": "^6.2.1",
    "streams": "^0.0.11",
    "url": "^0.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/uuid": "^8.3.4",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "build": "tsup-node --config ./tsup.config.ts",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  }
}
